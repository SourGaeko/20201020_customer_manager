{
  "name": "20201020_customer_manager",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000/",
  "main": "public/electron.js",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "electron-is-dev": "^1.2.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-scripts": "3.4.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-interface": "cross-env BROWSER=none npm run start",
    "start-electron": "electron .",
    "start-electron-after-interface": "wait-on http://localhost:3000 && npm run start-electron",
    "dev": "concurrently -n interface,app 'npm:start-interface' 'npm:start-electron-after-interface'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "electron": "^10.1.5",
    "wait-on": "^5.2.0"
  }
}
